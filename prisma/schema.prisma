generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum SellerCategory {
    Bar
    Brewer
}

enum ProductCategory {
    WhiteSpirit
    Rum
    Gin
}

model Seller {
    id               Int             @unique @default(autoincrement())
    place_name       String
    operating_time   String[]
    description      String
    latitude         Float
    longitude        Float
    address          String
    tel              String
    email            String
    category         SellerCategory
    product_category ProductCategory
    imges            String
}
